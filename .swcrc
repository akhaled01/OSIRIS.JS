{
  "jsc": {
    "parser": {
      "syntax": "ecmascript",
      "jsx": true
    },
    "transform": {
      "react": { // NOT USING REACT ON ALLAH BRO
        "runtime": "classic", // Set to "classic" to use a custom createElement
        "pragma": "createElement" // This tells SWC to use your custom createElement function
      }
    }
  },
  "module": {
    "type": "es6"
  }
}
